
set term png size 1024, 768
set out 'zero.png'

# set style data linespoints
set style data lines
set key left

set title "Excess/deficit of zeros of EG(gpf; z)"
set xlabel "radius"
set ylabel "num zeros minus radius"

# set xrange [0:1]
set xzeroaxis lw 2

plot "zero-count.dat" using 1:($2-$1) title "" lw 2
