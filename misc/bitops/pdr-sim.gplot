
set term png size 640, 480
set out 'pdr-sim.png'

set style data lines
set key left

set title "Expander Function pdr"
set xlabel "y"
set ylabel "pdr"


plot "pdr-sim-0.8.dat" using 2:3 title "pdr" linewidth 2, \
     "pdr-sim-0.8.dat" using 2:4 title "lo" linewidth 2, \
     "pdr-sim-0.8.dat" using 2:5 title "hi" linewidth 2, \
     "pdr-sim-0.8.dat" using 2:($5 - $4) title "hi-lo" linewidth 2
     # "pdr-sim-0.8.dat" using 2:(0.8*($6-0.5)) title "lo+hi" linewidth 2

