set ylabel "cpr"

plot "cpr-0.52.dat" using 2:3 title "K=0.52" linewidth 2, \
     "cpr-0.6.dat" using 2:3 title "K=0.6" linewidth 2, \
     "cpr-0.8.dat" using 2:3 title "K=0.8" linewidth 2, \
     "cpr-0.9.dat" using 2:3 title "K=0.9" linewidth 2

