
set term png size 1024, 768
set out 'growth.png'

set style data linespoints
# set style data lines
set key left

set title "the growth"
set xlabel "n"
set ylabel "t"

set xrange [30:]

plot "growth.dat" using 1:(log($5) / ($1 * 0.6931471805599453)) title "data", \
     "growth.dat" using 1:(0.3564 + log(log($1)) / 50 ) title "fit"



