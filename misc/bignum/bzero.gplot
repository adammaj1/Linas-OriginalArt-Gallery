
# set term x11
# set term gif size 640,480
# set out 'read.gif'

set term png small
set out 'bzero.png'

# set data style linespoints
set data style lines
set key right

# -------------------------------------------------------

set title "Graph of zero crossiings"
set xlabel "n"
set ylabel "zero"

f(x) = (0.25*3.1415926*x*x)

plot "bzero.dat" using 1:2 title "data" , \
     "bzero.dat" using 1:(f($1)) title "fit"


# pause 100

