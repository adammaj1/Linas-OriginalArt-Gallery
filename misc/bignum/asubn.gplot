
# set term x11
# set term gif size 640,480
# set out 'read.gif'

set term png small
set out 'asubn.png'
# set term postscript
# set out 'asubn.ps'

# set data style linespoints
set data style lines
set key right

set title "a_n the oscillatory term in zeta function"
set xlabel "n"
set ylabel "a_n)"

# set yrange [0:]


plot "asubn.dat" using 1:($2*log($1)/$1) title "", \
     "asubn.dat" using 1:(sin(3.14159*(-3.623+sqrt (3.623*3.623 - 4*(2.0-$1)/3.1416)))) title "guess"


# pause 100

