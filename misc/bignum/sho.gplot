
set term png small
set out 'sho.png'

# set data style linespoints
set data style lines
set key right

# set yrange [-1:1]
set xrange [-5:5]

set title "Simple Harmonic Oscillator"
set xlabel "x"
set ylabel "psi(x)"

plot "sho.dat" using 1:2 title "psi_1", \
     "sho.dat" using 1:3 title "psi_2"

