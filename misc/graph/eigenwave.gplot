set out 'eignewave.png'
set style data lines
set xzeroaxis lw 2

set title "Log of Totient wave function"
set xlabel "n"
set ylabel "-log(psi(n))"
set key left
set xrange [0:20]
plot "eigenwave.dat" using 1:3 lw 2 title "eigenwave", \
    "eigenwave.dat" using 1:(2.33*$1*log($1)) lw 2 title "fit"
