
set term png size 1024, 768
# set term png size 512, 384
set out 'slice.png'

# set style data linespoints
set style data lines
set key left

set title "Ingtegral of magnitude"
set xlabel "theta"
set ylabel "Amplitude"

# set xrange [0:1]
set yrange [0:6]

plot "slice-60K.dat" using ($2 / (2*3.141592653)) :3 title "steps" lw 2, \
     "slice-60K.dat" using ($2 / (2*3.141592653)) :(1.5*$2) title "line" lw 2
	
