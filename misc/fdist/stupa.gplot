
set term png small
set out 'stupa.png'

#################################################
# Used to generate the graph in the text
#
set term postscript
set out 'stupa.ps'

set data style lines
set key left
set border 3
set xtics nomirror
set ytics nomirror
set style line 1 lw 4

set xlabel "Stupa"

plot "stupa.dat" using 1:2 title "" ls 1


