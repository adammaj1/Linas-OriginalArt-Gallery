
# set term png small color
set term png small
set out 'wordcount.png'

# set data style linespoints
set data style lines
set key left

#################################################
# Used to generate the graph in the text
#
# set term postscript
# set out 'wordcount.ps'
set title "Word Frequency"
set xlabel "Word"
set ylabel "Count"
set logscale y
plot "wordcount.dat" using 1:2 title ""  with lines linewidth 3, \
	"wordcount.dat" using 1:3 title "uah"

